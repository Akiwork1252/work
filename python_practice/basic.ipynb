    "                        months = 0\n",
    "                    print(f'{f_year}年{f_month}月〜{l_year}年{l_month}月の{year}年と{months}ヶ月間の収入を記録します。')   \n",
    "                    break\n",
    "            else:\n",
    "                print('入力された値が正しくありません')\n",
    "        else:\n",
    "            print('入力が正しくないか、期間が長すぎます。')\n",
    "    return f_month, l_month, months\n",
    "\n",
